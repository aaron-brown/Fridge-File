<%= form_for :user, :url => users_path do |f| %>
  <div class="field" >
    <%= f.label :name %> 
    <%= f.text_field :name %>
  </div>
  
  <div class="field" >
    <%= f.label :email %> 
    <%= f.text_field :email %>
  </div>
  
  <div class="actions" >
    <%= f.submit "Sign up" %>
  </div>
<% end %>